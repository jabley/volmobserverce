<identification
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.volantis.com/xmlns/device-repository/device-identification"
        xsi:schemaLocation="http://www.volantis.com/xmlns/device-repository/device-identification http://www.volantis.com/schema/device-repository/v3.0/device-identification.xsd">
    <device name="Master"/>
    <device name="PocketPC2002-PIE">
        <userAgentPattern>
            <regularExpression>Mozilla/2\.0 (compatible; MSIE 3\.02; Windows CE; PPC.*</regularExpression>
        </userAgentPattern>
        <userAgentPattern>
            <regularExpression>Mozilla/2\.0 (compatible; MSIE 3\.02; Windows CE; 240x320; PPC).*</regularExpression>
        </userAgentPattern>
    </device>
    <device name="PocketPC2002-Phone">
        <headerPattern name="ua-voice">
            <regularExpression>TRUE.*PocketPC2002-PIE</regularExpression>
        </headerPattern>
    </device>
</identification>

