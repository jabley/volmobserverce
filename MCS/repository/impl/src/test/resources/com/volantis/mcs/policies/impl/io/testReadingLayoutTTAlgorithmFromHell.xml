<variable-policy xmlns="http://www.volantis.com/xmlns/2006/02/marlin-lpdm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mdv="http://www.volantis.com/xmlns/2004/12/meta-data-values" xmlns:rpdm="http://www.volantis.com/xmlns/2006/02/marlin-rpdm" xmlns:struct="http://www.volantis.com/xmlns/2005/10/ThemeStruct">
    <type>layout</type>
    <variants>
        <variant>
            <type>layout</type>
            <selection>
                <targeted>
                    <devices>
                        <device>Master</device>
                    </devices>
                </targeted>
            </selection>
            <content>
                <layout>
                    <canvasLayout>
                        <gridFormat rows="6" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100" columns="1">
                            <gridFormatColumns>
                                <gridFormatColumn width="100" widthUnits="percent"/>
                            </gridFormatColumns>
                            <gridFormatRow>
                                <paneFormat filterOnKeyboardUsability="0" backgroundColor="#ccccff" name="fileinfo" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                            </gridFormatRow>
                            <gridFormatRow>
                                <paneFormat filterOnKeyboardUsability="0" backgroundColor="#ccccff" name="header" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                            </gridFormatRow>
                            <gridFormatRow>
                                <paneFormat filterOnKeyboardUsability="0" backgroundColor="#ffffcc" name="test" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                            </gridFormatRow>
                            <gridFormatRow>
                                <gridFormat backgroundColor="#ffff99" rows="2" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100" columns="5">
                                    <gridFormatColumns>
                                        <gridFormatColumn width="20" widthUnits="percent"/>
                                        <gridFormatColumn width="20" widthUnits="percent"/>
                                        <gridFormatColumn width="20" widthUnits="percent"/>
                                        <gridFormatColumn width="20" widthUnits="percent"/>
                                        <gridFormatColumn width="20" widthUnits="percent"/>
                                    </gridFormatColumns>
                                    <gridFormatRow>
                                        <paneFormat filterOnKeyboardUsability="0" name="image1" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="image2" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="image3" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="image4" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="image5" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                    </gridFormatRow>
                                    <gridFormatRow>
                                        <paneFormat filterOnKeyboardUsability="0" name="test1" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="test2" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="test3" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="test4" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                        <paneFormat filterOnKeyboardUsability="0" name="test5" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                                    </gridFormatRow>
                                </gridFormat>
                            </gridFormatRow>
                            <gridFormatRow>
                                <paneFormat filterOnKeyboardUsability="0" name="reference" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                            </gridFormatRow>
                            <gridFormatRow>
                                <paneFormat filterOnKeyboardUsability="0" backgroundColor="#ccffcc" name="links" borderWidth="0" backgroundComponentType="image" widthUnits="percent" cellSpacing="0" cellPadding="0" optimizationLevel="never" width="100"/>
                            </gridFormatRow>
                        </gridFormat>
                    </canvasLayout>
                </layout>
            </content>
        </variant>
    </variants>
</variable-policy>
