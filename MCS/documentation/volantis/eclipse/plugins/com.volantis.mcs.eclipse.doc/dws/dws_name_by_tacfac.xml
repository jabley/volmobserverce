<?xml-stylesheet href="../xsl/reference2html.xsl" type="text/xsl"?>
<reference id="dws_name_by_tacfac" scheme="device">
   <body>
      <purpose>
         <p>Identifies the device from the eight digit combination of Type Allocation and Final Assembly codes (TAC FAC).
          </p>
         <p>If the number is missing or invalid, '404 Not Found' is returned.</p>
      </purpose>
      <method>GET</method>
      <syntax>/device/name-by-tacfac/[number]</syntax>
      <properties>
         <property id="headers">
            <name>number</name>
            <description>The TAC FAC number</description>
            <type>xs:string</type>
         </property>
      </properties>
      <examples>
         <p>Input</p>
         <codeblock><![CDATA[/device/name-by-tacfac/35209941]]></codeblock>
         <p>Returns</p>
         <codeblock><![CDATA[<device-name
   xmlns="http://www.volantis.com/xmlns/2006/01/device-ws">Alcatel-OT-332</device-name>]]></codeblock>
      </examples>
      <related-links>
         <linklist>
            <title>Related topics</title>
            <link href="dws_rest_api"/>
            <link href="dws_php_s5"/>
            <link href="dws_ruby_s5"/>
         </linklist>
      </related-links>
   </body>
</reference>
