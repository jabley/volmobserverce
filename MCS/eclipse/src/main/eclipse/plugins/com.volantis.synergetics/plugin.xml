<?eclipse version="${build.version.eclipse}"?>
<plugin
   id="com.volantis.synergetics"
   name="%pluginName"
   version="${version.plugins.mcs}"
   provider-name="%providerName">

   <runtime>
      <library name="synergetics.runtime-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.runtime-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.cornerstone-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.cornerstone-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.localization-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.metadata-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.metadata-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.old-cache-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.old-cache-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.net-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.net-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.performance-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.performance-arm-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.reporting-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.reporting-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.repository-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.repository-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.xml-utils-api-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
      <library name="synergetics.xml-utils-impl-${build.version.mcs}.jar">
          <export name="*"/>
      </library>
   </runtime>

    <requires>
        <import plugin="org.eclipse.core.runtime.compatibility"/>
        <import plugin="org.jakarta.log4j" version="${version.plugins.log4j}"/>
        <import plugin="org.jdom" version="${version.plugins.jdom}"/>
        <import plugin="org.jibx" version="${version.plugins.jibx}"/>
        <import plugin="javax.jdo" version="${version.plugins.jdo}"/>
        <import plugin="com.volantis.xml.xerces" version="${version.plugins.xml}"/>
    </requires>

</plugin>
