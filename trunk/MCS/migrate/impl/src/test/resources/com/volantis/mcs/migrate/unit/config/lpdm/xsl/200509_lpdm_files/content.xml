<lpdm:theme xmlns:lpdm="http://www.volantis.com/xmlns/2005/09/marlin-lpdm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.volantis.com/xmlns/2005/09/marlin-lpdm http://www.volantis.com/schema/2005/09/marlin-lpdm.xsd">
    <lpdm:deviceTheme deviceName="Master">
        <lpdm:rule>
            <lpdm:subjectSelectorSequence>
                <lpdm:classSelector class="normal"/>
            </lpdm:subjectSelectorSequence>
            <lpdm:styleProperties>
                <lpdm:content>
                    <lpdm:keyword>normal</lpdm:keyword>
                </lpdm:content>
            </lpdm:styleProperties>
        </lpdm:rule>
        <lpdm:rule>
            <lpdm:subjectSelectorSequence>
                <lpdm:classSelector class="string"/>
            </lpdm:subjectSelectorSequence>
            <lpdm:styleProperties>
                <lpdm:content>
                    <lpdm:list unique="false">
                        <lpdm:string>string</lpdm:string>
                    </lpdm:list>
                </lpdm:content>
            </lpdm:styleProperties>
        </lpdm:rule>
        <lpdm:rule>
            <lpdm:subjectSelectorSequence>
                <lpdm:classSelector class="whitespace"/>
            </lpdm:subjectSelectorSequence>
            <lpdm:styleProperties>
                <lpdm:content>
                    <lpdm:list unique="false">
                        <lpdm:string> </lpdm:string>
                    </lpdm:list>
                </lpdm:content>
            </lpdm:styleProperties>
        </lpdm:rule>
    </lpdm:deviceTheme>
</lpdm:theme>
