<base-url-policy xmlns="http://www.volantis.com/xmlns/2006/02/marlin-lpdm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.volantis.com/xmlns/2006/02/marlin-lpdm http://www.volantis.com/schema/2006/02/marlin-lpdm.xsd">
    <cache-control>
        <cache-this-policy>true</cache-this-policy>
        <retain-during-retry>true</retain-during-retry>
        <retry-failed-retrieval>false</retry-failed-retrieval>
        <retry-interval>10</retry-interval>
        <retry-max-count>5</retry-max-count>
        <time-to-live>100</time-to-live>
    </cache-control>
    <url>/welcome/images/</url>
    <location>context</location>
</base-url-policy>
