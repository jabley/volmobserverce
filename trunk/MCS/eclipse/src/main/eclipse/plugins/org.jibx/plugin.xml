<?eclipse version="${build.version.eclipse}"?>
<plugin
    id="org.jibx"
    name="%pluginName"
    version="${version.plugins.jibx}"
    provider-name="%providerName">

    <runtime>
      <library name="jibx-run-${version.plugins.jibx}.jar">
          <export name="*"/>
          <packages prefix="org.jibx"/>
      </library>
      <library name="xpp3-${version.plugins.jibx}.jar">
          <export name="*"/>
          <packages prefix="org.xmlpull"/>
      </library>
    </runtime>
</plugin>
