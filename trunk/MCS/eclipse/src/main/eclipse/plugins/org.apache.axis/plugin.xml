<?eclipse version="${build.version.eclipse}"?>
<plugin
   id="org.apache.axis"
   name="%pluginName"
   version="${version.plugins.axis}"
   provider-name="%providerName">

   <runtime>
      <library name="axis-${version.plugins.axis}.jar">
          <export name="*"/>
          <packages prefix="org.apache.axis"/>
      </library>
      <library name="wsdl4j-${version.plugins.axis}.jar">
          <export name="*"/>
	  <packages prefix="javax.wsdl"/>
      </library>
   </runtime>

    <requires>
        <import plugin="org.eclipse.core.runtime.compatibility"/>
        <import plugin="javax.xml.rpc" version="${version.plugins.xml-rpc}"/>
        <import plugin="org.apache.commons.logging" version="${version.plugins.commons-logging}"/>
        <import plugin="org.apache.commons.discovery" version="${version.plugins.commons-discovery}"/>
        <import plugin="javax.xml.qname" version="${version.plugins.xml-qname}"/>
        <import plugin="javax.xml.soap" version="${version.plugins.xml-soap}"/>
        <import plugin="com.volantis.xml.apis" version="${version.plugins.xml}"/>
        <import plugin="com.sun.jsse" version="${version.plugins.jsse}"/> 
    </requires>
</plugin>
