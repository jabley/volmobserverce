<?eclipse version="${build.version.eclipse}"?>
<plugin
   id="org.jakarta.log4j"
   name="%pluginName"
   version="${version.plugins.log4j}"
   provider-name="%providerName">

   <runtime>
      <library name="log4j-${version.plugins.log4j}.jar">
          <export name="*"/>
          <packages prefix="org.apache.log4j"/>
      </library>
   </runtime>
    
</plugin>
