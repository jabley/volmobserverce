  xmlns:widget="http://www.volantis.com/xmlns/2006/05/widget">

  <head>
    <title>Ajax Table Widget</title>
  </head>

  <body>
    <widget:table>
      <widget:tbody>
        <widget:load src="tbody-service"/>
        <tr><td>Please wait, loading...</td></tr>
      </widget:tbody>
    </widget:table>
