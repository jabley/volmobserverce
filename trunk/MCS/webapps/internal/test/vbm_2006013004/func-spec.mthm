<lpdm:theme xmlns:lpdm="http://www.volantis.com/xmlns/2005/12/marlin-lpdm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.volantis.com/xmlns/2005/12/marlin-lpdm
    /opt/work/2006013004/MCS/architecture/built/src/api/com/volantis/schema/2005/12/marlin-lpdm.xsd">
    <lpdm:deviceTheme deviceName="Master">
        <lpdm:rule>
            <lpdm:selectorSequence>
                <lpdm:class>articleGroup</lpdm:class>
            </lpdm:selectorSequence>
            <lpdm:properties>
                <lpdm:property name="mcs-container">
                    <lpdm:string>articleGroupPane</lpdm:string>
                </lpdm:property>
            </lpdm:properties>
        </lpdm:rule>
        <lpdm:rule>
            <lpdm:selectorSequence>
                <lpdm:class>article1</lpdm:class>
            </lpdm:selectorSequence>
            <lpdm:properties>
                <lpdm:property name="mcs-container">
                    <lpdm:string>articlePane1</lpdm:string>
                </lpdm:property>
            </lpdm:properties>
        </lpdm:rule>
        <lpdm:rule>
            <lpdm:selectorSequence>
                <lpdm:class>article2</lpdm:class>
            </lpdm:selectorSequence>
            <lpdm:properties>
                <lpdm:property name="mcs-container">
                    <lpdm:string>articlePane2</lpdm:string>
                </lpdm:property>
            </lpdm:properties>
        </lpdm:rule>
        <lpdm:rule>
            <lpdm:selectorSequence>
                <lpdm:class>article3</lpdm:class>
            </lpdm:selectorSequence>
            <lpdm:properties>
                <lpdm:property name="mcs-container">
                    <lpdm:string>articlePane3</lpdm:string>
                </lpdm:property>
            </lpdm:properties>
        </lpdm:rule>
    </lpdm:deviceTheme>
</lpdm:theme>
