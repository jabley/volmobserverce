<xs:schema targetNamespace="http://www.w3.org/2002/06/xhtml2" xmlns="http://www.w3.org/2002/06/xhtml2" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:include schemaLocation="xhtml2-text-module-base.xsd"/>
    <!--Internal Components-->
    <!--External Components-->
    <xs:element name="abbr" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>An abbreviation, including acronym.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="cite" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>A citation or source reference.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="code" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>Computer code.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="dfn" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>A definition.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="em" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>Emphasized.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="kbd" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>Input supplied by the user.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="quote" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
