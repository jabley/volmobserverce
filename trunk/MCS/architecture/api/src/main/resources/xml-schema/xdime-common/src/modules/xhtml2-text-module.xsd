        <xs:annotation>
            <xs:documentation>Emphasized (usually differently to &lt;em&gt;)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="sub" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>Subscript.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="sup" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>Superscript.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="var" type="TextCommon" substitutionGroup="TextContentSet">
        <xs:annotation>
            <xs:documentation>A program variable.</xs:documentation>
        </xs:annotation>
    </xs:element>
