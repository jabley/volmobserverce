<weblogic-web-app>
 <!--
  Configure Weblogic 8.1 and higher to prefer classes defined in the Web
  application. This prevents the known issue with Weblogic bundling an old
  version of javax.xml.soap.* and its own StAX implementation.

   See http://ws.apache.org/axis/java/install.html#WebLogic8.1 for more
   details
  -->
  <container-descriptor>
    <prefer-web-inf-classes>true</prefer-web-inf-classes>
  </container-descriptor>
</weblogic-web-app>
