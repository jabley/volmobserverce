<merge-report
        xmlns="http://www.volantis.com/xmlns/device/merge-report"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.volantis.com/xmlns/device/merge-report http://www.volantis.com/schema/device-repository/v3.0/device-merge-report.xsd"
        oldRevision="old revision" newRevision="new revision">
    <policy-definitions>
        <categories/>
        <policies/>
    </policy-definitions>
    <devices>
        <device name="PocketPC2002-PIE" status="updated">
            <hierarchy>
                <device name="Master"/>
                <device name="PC"/>
            </hierarchy>
            <identification>
                <header status="created">
                    <new>
                        <userAgentPattern>
                            <regularExpression>Mozilla/2\.0 (compatible; MSIE 3\.02; Windows CE; PPC.*</regularExpression>
                        </userAgentPattern>
                        <userAgentPattern>
                            <regularExpression>Mozilla/2\.0 (compatible; MSIE 3\.02; Windows CE; 240x320; PPC).*</regularExpression>
                        </userAgentPattern>
                    </new>
                </header>
            </identification>
            <policies/>
        </device>
        <device name="PocketPC2002-Phone" status="updated">
            <hierarchy>
                <device name="Master"/>
                <device name="PC"/>
            </hierarchy>
            <identification>
                <header status="created">
                    <new>
                        <headerPattern name="ua-voice">
                            <regularExpression>TRUE.*PocketPC2002-PIE</regularExpression>
                        </headerPattern>
                    </new>
                </header>
            </identification>
            <policies/>
        </device>
    </devices>
</merge-report>

