<definitions xmlns="http://www.volantis.com/xmlns/device-repository/policy-definitions" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.volantis.com/xmlns/device-repository/policy-definitions http://www.volantis.com/schema/device-repository/v3.0/device-policy-definitions.xsd">
    <device:types xmlns:device="http://www.volantis.com/xmlns/device-repository/policy-definitions">
        <device:type name="EmulateEmphasisTag">
            <device:structure>
                <device:field name="enable">
                    <device:type>
                        <device:boolean />
                    </device:type>
                </device:field>
                <device:field name="prefix">
                    <device:type>
                        <device:text />
                    </device:type>
                </device:field>
                <device:field name="suffix">
                    <device:type>
                        <device:text />
                    </device:type>
                </device:field>
                <device:field name="altTag">
                    <device:type>
                        <device:text />
                    </device:type>
                </device:field>
            </device:structure>
        </device:type>
    </device:types>
    <device:category xmlns:device="http://www.volantis.com/xmlns/device-repository/policy-definitions" name="custom" prefix="custom" />
</definitions>

