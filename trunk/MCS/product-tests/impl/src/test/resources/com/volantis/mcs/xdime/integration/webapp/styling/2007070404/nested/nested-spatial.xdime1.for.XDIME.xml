<html>
    <head>
        <title/>
    </head>
    <body>
        <spatial cols="2" style="width:100%">
            <spatial-row cols="2">
                <spatial-cell cols="2" style="background:#bbb;width:350px">
                    <grid cols="3" style="width:100%">
                        <grid-row cols="3">
                            <grid-cell cols="3" style="width:150px">
                                <pane>Pane 1.0</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 2.0</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 3.0</pane>
                            </grid-cell>
                        </grid-row>
                        <grid-row cols="3">
                            <grid-cell cols="3">
                                <pane>Pane 4.0</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 5.0</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 6.0 (Contains a lot of text that can cause line wrapping and can mess up the alignment of different cells. This helps illustrate the difference between aligned and unaligned spatial iterators.)</pane>
                            </grid-cell>
                        </grid-row>
                    </grid>
                </spatial-cell>
                <spatial-cell cols="2">
                    <grid cols="3" style="width:100%">
                        <grid-row cols="3">
                            <grid-cell cols="3" style="width:150px">
                                <pane>Pane 1.1</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 2.1</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 3.1</pane>
                            </grid-cell>
                        </grid-row>
                        <grid-row cols="3">
                            <grid-cell cols="3">
                                <pane>Pane 4.1</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 5.1</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 6.1</pane>
                            </grid-cell>
                        </grid-row>
                    </grid>
                </spatial-cell>
            </spatial-row>
            <spatial-row cols="2">
                <spatial-cell cols="2" style="background:#bbb">
                    <grid cols="3" style="width:100%">
                        <grid-row cols="3">
                            <grid-cell cols="3" style="width:150px">
                                <pane>Pane 1.2</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 2.2</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 3.2</pane>
                            </grid-cell>
                        </grid-row>
                        <grid-row cols="3">
                            <grid-cell cols="3">
                                <pane>Pane 4.2</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 5.2</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 6.2</pane>
                            </grid-cell>
                        </grid-row>
                    </grid>
                </spatial-cell>
                <spatial-cell cols="2">
                    <grid cols="3" style="width:100%">
                        <grid-row cols="3">
                            <grid-cell cols="3" style="width:150px">
                                <pane>Pane 1.3</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 2.3 (Contains a lot of text that can cause line wrapping and can mess up the alignment of different cells. This helps illustrate the difference between aligned and unaligned spatial iterators.)</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 3.3</pane>
                            </grid-cell>
                        </grid-row>
                        <grid-row cols="3">
                            <grid-cell cols="3">
                                <pane>Pane 4.3</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 5.3</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 6.3</pane>
                            </grid-cell>
                        </grid-row>
                    </grid>
                </spatial-cell>
            </spatial-row>
            <spatial-row cols="2" style="background:#555;color:#aaa">
                <spatial-cell cols="2">
                    <grid cols="3" style="width:100%">
                        <grid-row cols="3">
                            <grid-cell cols="3" style="width:150px">
                                <pane>Pane 1.4</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 2.4</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 3.4</pane>
                            </grid-cell>
                        </grid-row>
                        <grid-row cols="3">
                            <grid-cell cols="3">
                                <pane>Pane 4.4</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 5.4</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 6.4</pane>
                            </grid-cell>
                        </grid-row>
                    </grid>
                </spatial-cell>
                <spatial-cell cols="2">
                    <grid cols="3" style="width:100%">
                        <grid-row cols="3">
                            <grid-cell cols="3" style="width:150px">
                                <pane>Pane 1.5</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 2.5</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 3.5</pane>
                            </grid-cell>
                        </grid-row>
                        <grid-row cols="3">
                            <grid-cell cols="3">
                                <pane>Pane 4.5</pane>
                            </grid-cell>
                            <grid-cell cols="3" style="background:#eee">
                                <pane>Pane 5.5</pane>
                            </grid-cell>
                            <grid-cell cols="3">
                                <pane>Pane 6.5</pane>
                            </grid-cell>
                        </grid-row>
                    </grid>
                </spatial-cell>
            </spatial-row>
        </spatial>
    </body>
</html>
