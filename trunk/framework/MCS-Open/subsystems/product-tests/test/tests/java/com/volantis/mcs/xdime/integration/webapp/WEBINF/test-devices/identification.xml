<device:identification xmlns:device="http://www.volantis.com/xmlns/device-repository/device-identification" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.volantis.com/xmlns/device-repository/device-identification http://www.volantis.com/schema/device-repository/v3.0/device-identification.xsd">
    <device:device name="Master" />
    <device:device name="PC">
        <device:userAgentPattern>
            <device:regularExpression>Mozilla/3.*</device:regularExpression>
        </device:userAgentPattern>
        <device:userAgentPattern>
            <device:regularExpression>Mozilla/5.*</device:regularExpression>
        </device:userAgentPattern>
        <device:userAgentPattern>
            <device:regularExpression>Mozilla/4.*</device:regularExpression>
        </device:userAgentPattern>
        <device:userAgentPattern>
            <device:regularExpression>Mozilla/2.*</device:regularExpression>
        </device:userAgentPattern>
        <device:userAgentPattern>
            <device:regularExpression>Mozilla/1.*</device:regularExpression>
        </device:userAgentPattern>
    </device:device>
    <device:device name="WAP-Handset">
        <device:userAgentPattern>
            <device:regularExpression>WAP-Handset</device:regularExpression>
        </device:userAgentPattern>
    </device:device>
    <device:device name="XDIME">
        <device:userAgentPattern>
            <device:regularExpression>XDIME</device:regularExpression>
        </device:userAgentPattern>
    </device:device>
</device:identification>
