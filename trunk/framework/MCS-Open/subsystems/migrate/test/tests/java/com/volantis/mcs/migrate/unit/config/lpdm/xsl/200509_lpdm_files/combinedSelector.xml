<lpdm:theme xmlns:lpdm="http://www.volantis.com/xmlns/2005/09/marlin-lpdm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.volantis.com/xmlns/2005/09/marlin-lpdm http://www.volantis.com/schema/2005/09/marlin-lpdm.xsd">
    <lpdm:deviceTheme deviceName="Master">
        <lpdm:rule>
            <lpdm:combinedSelector combinator="child">
                <lpdm:contextualSelectorSequence>
                    <lpdm:classSelector class="raphael"/>
                </lpdm:contextualSelectorSequence>
                <lpdm:subjectSelectorSequence>
                    <lpdm:classSelector class="donatello"/>
                </lpdm:subjectSelectorSequence>
            </lpdm:combinedSelector>
            <lpdm:combinedSelector combinator="child">
                <lpdm:contextualSelectorSequence>
                    <lpdm:classSelector class="raphael"/>
                </lpdm:contextualSelectorSequence>
                <lpdm:combinedSelector combinator="descendant">
                    <lpdm:contextualSelectorSequence>
                        <lpdm:classSelector class="donatello"/>
                    </lpdm:contextualSelectorSequence>
                    <lpdm:subjectSelectorSequence>
                        <lpdm:classSelector class="leonardo"/>
                    </lpdm:subjectSelectorSequence>
                </lpdm:combinedSelector>
            </lpdm:combinedSelector>
            <lpdm:styleProperties>
                <lpdm:background-color>
                    <lpdm:colorRGB>#DEFACE</lpdm:colorRGB>
                </lpdm:background-color>
            </lpdm:styleProperties>
        </lpdm:rule>
    </lpdm:deviceTheme>
</lpdm:theme>
