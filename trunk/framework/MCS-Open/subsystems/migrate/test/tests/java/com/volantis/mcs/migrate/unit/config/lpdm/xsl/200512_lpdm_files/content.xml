<theme xmlns="http://www.volantis.com/xmlns/2005/12/marlin-lpdm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.volantis.com/xmlns/2005/12/marlin-lpdm http://www.volantis.com/schema/2005/12/marlin-lpdm.xsd">
    <deviceTheme deviceName="Master">
        <rule>
            <selectorSequence>
                <class>normal</class>
            </selectorSequence>
            <properties>
                <property name="content">
                    <keyword>normal</keyword>
                </property>
            </properties>
        </rule>
        <rule>
            <selectorSequence>
                <class>string</class>
            </selectorSequence>
            <properties>
                <property name="content">
                    <list unique="false">
                        <string>string</string>
                    </list>
                </property>
            </properties>
        </rule>
        <rule>
            <selectorSequence>
                <class>whitespace</class>
            </selectorSequence>
            <properties>
                <property name="content">
                    <list unique="false">
                        <string> </string>
                    </list>
                </property>
            </properties>
        </rule>
    </deviceTheme>
</theme>
