<userInput>
  <panel order="0">
    <field type="staticText" align="left" txt="Choose Repository Vendor" id="repository.title" size="4" bold="true"/>
    <field type="divider"/>
    <field type="space"/>
    <field type="combo" variable="mcsRepositoryVendor">
      <spec txt="Select initial repository vendor" id="repository.vendor.prompt" >
        <choice processor="" id="repository.vendor.derby" txt="Derby or Cloudscape" value="derby"/>
        <choice processor="" id="repository.vendor.db2" txt="IBM DB2" value="db2"/>
        <choice processor="" id="repository.vendor.db2-type4" txt="IBM DB2 Type4" value="db2-type4"/>
        <choice processor="" id="repository.vendor.datad" txt="MSSQL Server (DataD)" value="mssql-datad"/>
        <choice processor="" id="repository.vendor.jsql" txt="MSSQL Server (JSQL)" value="mssql-jsql"/>
        <choice processor="" id="repository.vendor.ms" txt="MSSQL Server (MS)" value="mssql-ms"/>
        <choice processor="" id="repository.vendor.ms2005" txt="MSSQL Server (MS2005)" value="mssql-2005"/>
        <choice processor="" id="repository.vendor.mysql" txt="MySQL" value="mysql"/>
        <choice processor="" id="repository.vendor.odbc" txt="ODBC" value="odbc"/>
        <choice processor="" id="repository.vendor.oracle" txt="Oracle" value="oracle" set="true"/>
        <choice processor="" id="repository.vendor.postgres" txt="Postgres" value="postgres"/>
        <choice processor="" id="repository.vendor.xml" txt="XML" value="xml"/>
      </spec>
    </field>
  </panel>
  <panel order="1">
    <field type="staticText" align="left" txt="Configure repository connection" id="repository.connection.title" size="4" bold="true"/>
    <field type="divider"/>
    <field type="space"/>
    <field type="text" variable="mcsRepositoryHost">
      <spec txt="Database Server Host:" id="repository.host.label" size="20" set="myhost"/>
    </field>
    <field type="text" variable="mcsRepositoryPort">
      <spec txt="Database Server Port:" id="repository.port.label" size="5" set="1521"/>
    </field>
    <field type="text" variable="mcsRepositorySource">
      <spec txt="Database:" id="repository.source.label" size="20" set="mysource"/>
    </field>
    <field type="text" variable="mcsRepositoryUser">
      <spec txt="Username:" id="repository.user.label" size="20" set="mariner"/>
    </field>
    <field type="text" variable="mcsRepositoryPassword">
      <spec txt="Password:" id="repository.password.label" size="20" set="mariner"/>
    </field>
    <field type="text" variable="mcsProjectName">
      <spec txt="Project:" id="repository.projectname.label" size="20" set="NamedProject"/>
    </field>
  </panel>
  <panel order="2">
    <field type="staticText" align="left" txt="Choose Web Application Server" id="webserver.title" size="4" bold="true"/>
    <field type="divider"/>
    <field type="space"/>
    <field type="combo" variable="mcsWebServer">
      <spec txt="Select server type" id="webserver.server.type" >
        <choice processor="" id="webserver.server.tomcat4" txt="Tomcat41" value="Tomcat41" set="true"/>
        <choice processor="" id="webserver.server.weblogic92" txt="Weblogic92" value="Weblogic92"/>
        <choice processor="" id="webserver.server.websphere60" txt="Websphere60" value="Websphere60"/>
        <choice processor="" id="webserver.server.jboss3" txt="JBoss" value="Tomcat31"/>
        <choice processor="" id="webserver.server.other" txt="other" value="Generic"/>
      </spec>
    </field>
    <field type="space"/>
    <field type="radio" variable="mcsWebServerJspType">
      <spec txt="Select server type" id="webserver.server.jspcompatibility" >
        <choice id="webserver.server.jsp11" txt="JSP 1.1" value="jsp11"/>
        <choice id="webserver.server.jsp12" txt="JSP 1.2" value="jsp12" set="true"/>
      </spec>
    </field>
    <field type="space"/>
    <field type="text" variable="mcsWebServerHost">
      <description align="left" txt="The name of the host on which mcs is to run" id="webserver.host.description"/>
      <spec txt="Host:" id="webserver.host.label" size="20" set="localhost"/>
    </field>
    <field type="space"/>
    <field type="text" variable="mcsWebServerPort">
      <description align="left" txt="The port of the host on which mcs is to run" id="webserver.port.description"/>
      <spec txt="Port:" id="webserver.port.label" size="6" set="8080"/>
    </field>
  </panel>
  <panel order="3">
    <field type="staticText" align="left" txt="Configure Media Access Proxy Host" id="maphost.title" size="4" bold="true"/>
    <field type="divider"/>
    <field type="space"/>
    <field type="text" variable="mcsMAPHost">
      <description align="left" txt="The URL of the media access proxy" id="maphost.host.description"/>
      <spec txt="MAP URL:" id="maphost.host.label" size="25" set="http://host:port/map/map"/>
