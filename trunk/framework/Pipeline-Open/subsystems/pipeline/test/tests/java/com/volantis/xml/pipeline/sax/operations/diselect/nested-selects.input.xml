<?xml version="1.0"?>
<root>
    <diselect:select
        xmlns:diselect="http://www.w3.org/2004/06/diselect">
        <diselect:when expr="true()">

            <diselect:select
                xmlns:diselect="http://www.w3.org/2004/06/diselect">
                <diselect:when expr="true()">

                    <diselect:select
                        xmlns:diselect="http://www.w3.org/2004/06/diselect">
                        <diselect:when expr="true()">
                            <showme/>
                        </diselect:when>
                        <diselect:when expr="false()">
                            <skip/>
                        </diselect:when>
                    </diselect:select>

                </diselect:when>
            </diselect:select>

        </diselect:when>
    </diselect:select>
</root>