<device xmlns="http://www.volantis.com/xmlns/device-repository/device" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.volantis.com/xmlns/device-repository/device ../../../xml-schema/device.xsd" name="TestDevice">
    <policies>
        <policy name="basicDevice" value="true">
            <standard>
                <policy name="basicDevice" value="false"/>
            </standard>
        </policy>
        <policy name="dateint" value="2000"/>
        <policy name="entryType" value="real_device"/>
        <policy name="firmversion" value="1.0"/>
        <policy name="localsec" value="50"/>
        <policy name="modelnum" value="7110"/>
        <policy name="prodname" value="7110"/>
        <policy name="protocol.wml.emulate.bigTag.enable" value="true"/>
        <policy name="protocol.wml.emulate.bigTag.prefix" value="&lt;"/>
        <policy name="protocol.wml.emulate.bigTag.suffix" value="&gt;"/>
        <policy name="protocol.wml.emulate.bigTag.altTag" value="i"/>
        <policy name="protocol.wml.emulate.bigTag">
            <field name="enable" value="true"/>
            <field name="prefix" value="&lt;"/>
            <field name="suffix" value="&gt;"/>
            <field name="altTag" value="i"/>
            <field name="empty" value=""/>
        </policy>
        <policy name="rendermode" value="greyscale"/>
        <policy name="revision" value="&gt;= 4.69"/>
        <policy name="ssl" value="true"/>
        <policy name="sslkeystrng" value="128"/>
        <policy name="sslvers" value="WTLS"/>
        <policy name="UAProf.AcceptDownloadableSoftware" value="false"/>
        <policy name="UAProf.CcppAccept">
            <value>text/html</value>
            <value>text/plain</value>
            <value>image/jpeg</value>
        </policy>
        <policy name="UAProf.TablesCapable" value="false"/>
        <policy name="UAProf.WapPushMsgSize" value="2000"/>
        <policy name="wtlsclass" value="false"/>
        <policy name="testinherit"  value="128">
            <inherit/>
        </policy>
        <policy name="wtlskeystrng" value="128"/>
    </policies>
</device>
