<lpdm:layoutFormat xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.volantis.com/xmlns/2005/12/marlin-lpdm
    /opt/work/2006013004/MCS/architecture/built/src/api/com/volantis/schema/2005/12/marlin-lpdm.xsd"
    xmlns:lpdm="http://www.volantis.com/xmlns/2005/12/marlin-lpdm">
    <lpdm:deviceLayoutCanvasFormat deviceName="Master" defaultFragment="fragment1">
        <lpdm:fragmentFormat name="fragment1" linkText="Link to Main Page">
            <lpdm:gridFormat rows="4" borderWidth="0" backgroundComponentType="image"
                widthUnits="pixels" horizontalAlignment="left" cellSpacing="0" cellPadding="0"
                optimizationLevel="never" verticalAlignment="center" columns="1">
                <lpdm:gridFormatColumns>
                    <lpdm:gridFormatColumn widthUnits="percent"/>
                </lpdm:gridFormatColumns>
                <lpdm:gridFormatRow>
                    <lpdm:paneFormat filterOnKeyboardUsability="0" name="articleGroupPane"
                        borderWidth="0" backgroundComponentType="image"
                        widthUnits="percent" horizontalAlignment="left" cellSpacing="0"
                        cellPadding="0" optimizationLevel="never"
                        verticalAlignment="center" width="100"/>
                </lpdm:gridFormatRow>
                <lpdm:gridFormatRow>
                    <lpdm:fragmentFormat name="articleFragment1" linkText="Link to Articles">
                        <lpdm:gridFormat rows="1" borderWidth="0" backgroundComponentType="image"
                            widthUnits="percent" horizontalAlignment="left" cellSpacing="0"
                            cellPadding="0" optimizationLevel="never" verticalAlignment="center"
                            columns="1">
                            <lpdm:gridFormatColumns>
                                <lpdm:gridFormatColumn widthUnits="percent"/>
                            </lpdm:gridFormatColumns>
                            <lpdm:gridFormatRow>
                                <lpdm:paneFormat filterOnKeyboardUsability="0" name="articlePane1"
                                    borderWidth="0" backgroundComponentType="image"
                                    widthUnits="percent" horizontalAlignment="left" cellSpacing="0"
                                    cellPadding="0" optimizationLevel="never"
                                    verticalAlignment="center" width="100"/>
                            </lpdm:gridFormatRow>
                        </lpdm:gridFormat>
                    </lpdm:fragmentFormat>
                </lpdm:gridFormatRow>
                <lpdm:gridFormatRow>
                    <lpdm:fragmentFormat name="articleFragment2" linkText="Link to Articles">
                        <lpdm:gridFormat rows="1" borderWidth="0" backgroundComponentType="image"
                            widthUnits="percent" horizontalAlignment="left" cellSpacing="0"
                            cellPadding="0" optimizationLevel="never" verticalAlignment="center"
                            columns="1">
                            <lpdm:gridFormatColumns>
                                <lpdm:gridFormatColumn widthUnits="percent"/>
                            </lpdm:gridFormatColumns>
                            <lpdm:gridFormatRow>
                                <lpdm:paneFormat filterOnKeyboardUsability="0" name="articlePane2"
                                    borderWidth="0" backgroundComponentType="image"
                                    widthUnits="percent" horizontalAlignment="left" cellSpacing="0"
                                    cellPadding="0" optimizationLevel="never"
                                    verticalAlignment="center" width="100"/>
                            </lpdm:gridFormatRow>
                        </lpdm:gridFormat>
                    </lpdm:fragmentFormat>
                </lpdm:gridFormatRow>
                <lpdm:gridFormatRow>
                    <lpdm:fragmentFormat name="articleFragment3" linkText="Link to Articles">
                        <lpdm:gridFormat rows="1" borderWidth="0" backgroundComponentType="image"
                            widthUnits="percent" horizontalAlignment="left" cellSpacing="0"
                            cellPadding="0" optimizationLevel="never" verticalAlignment="center"
                            columns="1">
                            <lpdm:gridFormatColumns>
                                <lpdm:gridFormatColumn widthUnits="percent"/>
                            </lpdm:gridFormatColumns>
                            <lpdm:gridFormatRow>
                                <lpdm:paneFormat filterOnKeyboardUsability="0" name="articlePane3"
                                    borderWidth="0" backgroundComponentType="image"
                                    widthUnits="percent" horizontalAlignment="left" cellSpacing="0"
                                    cellPadding="0" optimizationLevel="never"
                                    verticalAlignment="center" width="100"/>
                            </lpdm:gridFormatRow>
                        </lpdm:gridFormat>
                    </lpdm:fragmentFormat>
                </lpdm:gridFormatRow>
            </lpdm:gridFormat>
        </lpdm:fragmentFormat>
    </lpdm:deviceLayoutCanvasFormat>
</lpdm:layoutFormat>
