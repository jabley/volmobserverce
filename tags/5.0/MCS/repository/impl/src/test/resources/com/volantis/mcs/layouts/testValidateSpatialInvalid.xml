<variable-policy xmlns="http://www.volantis.com/xmlns/2006/02/marlin-lpdm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mdv="http://www.volantis.com/xmlns/2004/12/meta-data-values" xmlns:rpdm="http://www.volantis.com/xmlns/2006/02/marlin-rpdm" xmlns:struct="http://www.volantis.com/xmlns/2005/10/ThemeStruct">
    <type>layout</type>
    <variants>
        <variant>
            <type>layout</type>
            <selection>
                <targeted>
                    <devices>
                        <device>Master</device>
                    </devices>
                </targeted>
            </selection>
            <content>
                <layout>
                    <canvasLayout>
                        <spatialFormatIterator
                            name="1format"

                            backgroundColor="grungy"
                            backgroundComponentType="evil"
                            borderWidth="-1"
                            cellPadding="-2"
                            cellSpacing="-3"
                            height="-4"
                            horizontalAlignment="asleep"
                            verticalAlignment="awake"
                            width="minus five"
                            widthUnits="dress-size"
                            directionality="l 2r"
                            optimizationLevel="insane"

                            styleClass="2style"

                            indexingDirection="over-the-top"
                            rows="random"
                            rowCount="-6"
                            rowStyleClasses="a 1 and a 2"
                            columns="wonky"
                            columnCount="-7"
                            columnStyleClasses="a 2 and a 3"
                            alignContent="maybe">

                            <emptyFormat/>
                        </spatialFormatIterator>
                    </canvasLayout>
                </layout>
            </content>
        </variant>
    </variants>
</variable-policy>
