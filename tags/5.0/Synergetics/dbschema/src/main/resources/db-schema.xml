<project name="DDL scriprts" basedir="." default="dist">

    <target name="create-ddls"
        description="Generates files sql scripts for creation of tables all databases.
            Passed parameters:
                param1 is table definitions xml file;
                param2 is way to dirs with sql scripts;
                param3 is full way to dirs with xslt scripts
                param4 is name of calling product(lower case)"
        depends="check-uptodate">

        <antcall target="make-dirs">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>

        <antcall target="generate-oracle9i">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-pre-oracle9i">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-mysql">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-postgres">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-db2">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-db2-zos">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-mssql">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-sql92">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
        <antcall target="generate-sybase">
            <param name="table" value="${param1}"/>
            <param name="generated.sql.dir" value="${param2}"/>
            <param name="synergetics.schema.dir" value="${param3}"/>
            <param name="product.name" value="${param4}"/>
        </antcall>
    </target>

    <target name="clean-ddls"
        description="clean all generated script files">

        <antcall target="drop-oracle9i">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-pre-oracle9i">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-mysql">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-postgres">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-db2">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-db2-zos">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-mssql">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-sql92">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>
        <antcall target="drop-sybase">
            <param name="generated.sql.dir" value="${param2}"/>
        </antcall>

    </target>

    <target name="check-uptodate"
        description="initializes all the skip properties"
        depends="check-available,
            check-uptodate-oracle9i,
            check-uptodate-pre-oracle9i,
            check-uptodate-mysql,
            check-uptodate-postgres,
            check-uptodate-db2,
            check-uptodate-db2-zos,
            check-uptodate-mssql,
            check-uptodate-sql92,
            check-uptodate-sybase"/>

    <target name="check-available"
        description="checked target by available of sql files, if one is existe">

        <available file="${param2}/oracle9i/create_${param4}_tables.sql" property="oracle9i.available"/>
        <available file="${param2}/pre-oracle9i/create_${param4}_tables.sql" property="pre-oracle9i.available"/>
        <available file="${param2}/mysql/create_${param4}_tables.sql" property="mysql.available"/>
        <available file="${param2}/postgres/create_${param4}_tables.sql" property="postgres.available"/>
        <available file="${param2}/db2/create_${param4}_tables.sql" property="db2.available"/>
        <available file="${param2}/db2/zosdb2_71/create_${param4}_tables.sql" property="db2-zos.available"/>
        <available file="${param2}/mssql/create_${param4}_tables.sql" property="mssql.available"/>
        <available file="${param2}/sql92/create_${param4}_tables.sql" property="sql92.available"/>
        <available file="${param2}/sybase/create_${param4}_tables.sql" property="sybase.available"/>

        <condition property="generate-oracle9i.must.build">
            <not>
                <isset property="oracle9i.available"/>
            </not>
        </condition>
        <condition property="generate-pre-oracle9i.must.build">
            <not>
                <isset property="pre-oracle9i.available"/>
            </not>
        </condition>
        <condition property="generate-mysql.must.build">
            <not>
                <isset property="mysql.available"/>
            </not>
        </condition>
        <condition property="generate-postgres.must.build">
            <not>
                <isset property="postgres.available"/>
            </not>
        </condition>
        <condition property="generate-db2.must.build">
            <not>
                <isset property="db2.available"/>
            </not>
        </condition>
        <condition property="generate-db2-zos.must.build">
            <not>
                <isset property="db2-zos.available"/>
            </not>
        </condition>
        <condition property="generate-mssql.must.build">
            <not>
                <isset property="mssql.available"/>
            </not>
        </condition>
        <condition property="generate-sql92.must.build">
            <not>
                <isset property="sql92.available"/>
            </not>
        </condition>
        <condition property="generate-sybase.must.build">
            <not>
                <isset property="sybase.available"/>
            </not>
        </condition>

    </target>


    <target name="check-uptodate-oracle9i"
        description="initializes the skip properties"
        if="oracle9i.available">

        <uptodate property="generate-oracle9i.compare.create.xslt"
            srcfile="${param2}/oracle9i/create_${param4}_tables.sql"
            targetfile="${param3}/CreateTableOracle.xslt"/>

        <uptodate property="generate-oracle9i.compare.xml"
            srcfile="${param2}/oracle9i/create_${param4}_tables.sql"
            targetfile="${param1}"/>

        <uptodate property="generate-oracle9i.compare.drop.xslt"
            srcfile="${param2}/oracle9i/drop_${param4}_tables.sql"
            targetfile="${param3}/DropOracle.xslt"/>

        <condition property="generate-oracle9i.must.build">
            <or>
                <isset property="generate-oracle9i.compare.create.xslt"/>
                <isset property="generate-oracle9i.compare.xml"/>
                <isset property="generate-oracle9i.compare.drop.xslt"/>
            </or>
        </condition>

    </target>

