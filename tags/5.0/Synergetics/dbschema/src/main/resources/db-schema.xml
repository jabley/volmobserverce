    <target name="make-dirs">
        <mkdir dir="${generated.sql.dir}"/>
        <mkdir dir="${generated.sql.dir}/db2"/>
        <mkdir dir="${generated.sql.dir}/db2/zosdb2_71"/>
        <mkdir dir="${generated.sql.dir}/mssql"/>
        <mkdir dir="${generated.sql.dir}/mysql"/>
        <mkdir dir="${generated.sql.dir}/oracle9i"/>
        <mkdir dir="${generated.sql.dir}/pre-oracle9i"/>
        <mkdir dir="${generated.sql.dir}/sql92"/>
        <mkdir dir="${generated.sql.dir}/sybase"/>
        <mkdir dir="${generated.sql.dir}/postgres"/>
    </target>

    <target name="generate-oracle9i"
        description="Generate the Oracle SQL"
        if="generate-oracle9i.must.build">

        <delete>
            <fileset dir="${generated.sql.dir}/oracle9i" includes="*"/>
        </delete>

        <xslt in="${table}"
            out="${generated.sql.dir}/oracle9i/create_${product.name}_tables.sql"
            style="${synergetics.schema.dir}/CreateTableOracle.xslt">
            <param name="comments" expression="true"/>
            <param name="version" expression="true"/>
        </xslt>

        <xslt in="${table}"
            out="${generated.sql.dir}/oracle9i/drop_${product.name}_tables.sql"
            style="${synergetics.schema.dir}/DropOracle.xslt">
            <param name="comments" expression="true"/>
        </xslt>

    </target>

    <target name="generate-pre-oracle9i"
        description="Generate the Oracle SQL"
        if="generate-pre-oracle9i.must.build">

        <delete>
            <fileset dir="${generated.sql.dir}/pre-oracle9i" includes="*"/>
        </delete>

        <xslt in="${table}"
            out="${generated.sql.dir}/pre-oracle9i/create_${product.name}_tables.sql"
            style="${synergetics.schema.dir}/CreateTableOracle.xslt">
            <param name="version" expression="false"/>
            <param name="comments" expression="true"/>
        </xslt>

        <xslt in="${table}"
            out="${generated.sql.dir}/pre-oracle9i/drop_${product.name}_tables.sql"
            style="${synergetics.schema.dir}/DropOracle.xslt">
            <param name="comments" expression="true"/>
        </xslt>

    </target>

